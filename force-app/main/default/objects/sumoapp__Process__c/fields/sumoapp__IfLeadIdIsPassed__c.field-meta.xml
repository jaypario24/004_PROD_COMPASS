<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>sumoapp__IfLeadIdIsPassed__c</fullName>
    <deprecated>false</deprecated>
    <externalId>false</externalId>
    <label>If LEAD ID is passed</label>
    <required>false</required>
    <trackTrending>false</trackTrending>
    <type>Picklist</type>
    <valueSet>
        <restricted>true</restricted>
        <valueSetDefinition>
            <sorted>false</sorted>
            <value>
                <fullName>SKIP_LEAD_FORM</fullName>
                <default>true</default>
                <label>Skip the Lead Form - Skip the Lead Form when a Lead ID is passed in the URL.</label>
            </value>
            <value>
                <fullName>PREPOPULATE_LEAD_FORM</fullName>
                <default>false</default>
                <label>Prepopulate the Lead Form - Pre-Populate the Lead Form when a Lead ID is passed in the URL and update the lead with any changes made in the lead form data.</label>
